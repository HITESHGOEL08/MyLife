{
  "name": "my-profilo",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "proxy": "http://localhost:19975",
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@reduxjs/toolkit": "^2.11.2",
    "axios": "^1.13.5",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.1.14",
    "prop-types": "^15.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.13.0",
    "react-scripts": "5.0.1",
    "redux": "^5.0.1",
    "redux-saga": "^1.4.2",
    "redux-thunk": "^3.1.0",
    "sass": "^1.97.3"
  },
  "scripts": {
    "build": "npm-run-all -p build-js",
    "start": "npm-run-all -p start-backserver-js start-js",
    "start-backserver-js": "nodemon server/index.js",
    "start-backserver": "node server/index.js",
    "start-js": "set HTTPS=true&&react-scripts start",
    "build-js": "react-scripts build",
    "test": "react-scripts test -u",
    "deploy": "npm-run-all -p build && gh-pages -d build",
    "deploy-workflow": "yarn install && npm-run-all -p build && gh-pages -d build",
    "eject": "react-scripts eject",
    "test:watch": "test --watch",
    "test:coverage": "react-scripts test --coverage --runInBand --watchAll=false",
    "test:win": "set CI=true && react-scripts test --silent --env=jsdom --coverage --testResultsProcessor jest-junit -u"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "!src/index.tsx"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "lcov"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.28.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.0",
    "@types/node-sass": "^4.11.8",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.3.3",
    "css-loader": "^7.1.4",
    "gh-pages": "^6.3.0",
    "lodash": "^4.17.23",
    "moment": "^2.30.1",
    "npm-run-all": "^4.1.5",
    "react-ga": "^3.3.1",
    "react-test-renderer": "^19.2.4",
    "sass-loader": "^16.0.7",
    "style-loader": "^4.0.0",
    "typescript": "~5.9.3",
    "web-vitals": "^5.1.0"
  }
}
